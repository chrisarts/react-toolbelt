{
  "name": "@react-toolbelt/helpers",
  "version": "1.0.0",
  "description": "Helpers for common utilities",
  "homepage": "https://github.com/chrisarts/react-toolbelt#readme",
  "bugs": {
    "url": "https://github.com/chrisarts/react-toolbelt/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisarts/react-toolbelt",
    "directory": "packages/helpers"
  },
  "license": "MIT",
  "author": "Cristhian Gutierrez",
  "exports": {
    ".": {
      "types": "./build/dts/index.d.ts",
      "import": "./build/esm/index.js",
      "default": "./build/cjs/index.js"
    }
  },
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "types": "./build/dts/index.d.ts",
  "files": [
    "build",
    "src",
    "KeyMap",
    "react",
    "server",
    "tree",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "npx twin pack-dev",
    "build:watch": "npx twin pack-dev -w",
    "clean": "rm -rf build",
    "dev": "tsc -p tsconfig.build.json --watch --preserveWatchOutput",
    "lint": "biome lint",
    "test": "npx vitest"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "provenance": true
  }
}
